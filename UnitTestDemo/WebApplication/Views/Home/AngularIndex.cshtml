@{
    ViewBag.Title = "Home Page";
}
@model IEnumerable<MyObject>

<div class="jumbotron">
    <h1>Unit Testing</h1>
    <p class="lead">Unit testing is fun! All the cool kids are doing it.</p>
    <p><a href="http://www.manning.com/osherove/" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row" ng-app="app" ng-controller="myObjectCtrl">
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Created</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="item in list">
                <td>{id}</td>
                <td>{name}</td>
                <td>{created}</td>
            </tr>
        </tbody>
    </table>
    <a href="@Url.Action("Add")" class="btn btn-lg btn-success"
       data-toggle="modal"
       data-target="#addObjectModal">Add Object</a>
    <div id="addObjectModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="addObjectLabel" aria-hidden="true"></div>
</div>